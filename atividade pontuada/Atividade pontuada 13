#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <string.h>
#include <ctype.h>

int main()
{
    setlocale(LC_ALL, "portuguese");
    int numero1;
    int numero2;
  
  printf("digite um número : \n");
  scanf("%d", &numero1);
  
  printf("digite um número : \n");
  scanf("%d", &numero2);
  
    system("cls || clear");
    
   if(numero1 > numero2)
   {
    printf("Maior numero: %d \n", numero1);
    printf("Menor numero: %d \n", numero2);   
   }
   else
   {  
    printf("Maior numero: %d \n", numero2);
    printf("Menor numero: %d \n", numero1);
   }
    return 0;
}
    
    
    
    
